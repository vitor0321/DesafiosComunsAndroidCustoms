<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:color="@color/chip_text_color_dark" android:state_enabled="true" android:state_selected="true"/>
    <item android:color="?attr/colorPrimary" android:state_enabled="true" android:state_checked="true"/>

    <!-- 85% opacity. -->
    <item android:alpha="0.85" android:color="?attr/colorOnSurface" android:state_enabled="true"/>
    <item android:alpha="0.35" android:color="?attr/colorOnSurface"/> <!-- 35% of 85% opacity. -->

</selector>
